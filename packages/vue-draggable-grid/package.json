{
  "name": "@noction/vue-draggable-grid",
  "version": "1.9.10",
  "description": "Vue3 grid layout with resize, drag and responsive",
  "keywords": [
    "grid",
    "grid-layout",
    "layout",
    "resize",
    "drag",
    "vue3",
    "vue",
    "composition-api",
    "typescript"
  ],
  "author": {
    "name": "Victor Poltoratchi",
    "email": "poltoratchi1369@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Noction/draggable-grid",
    "directory": "packages/vue-draggable-grid"
  },
  "bugs": {
    "url": "https://github.com/Noction/draggable-grid/issues",
    "email": "poltoratchi1369@gmail.com"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "dist/vue-draggable-grid.mjs",
  "module": "dist/vue-draggable-grid.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-draggable-grid.mjs",
      "default": "./dist/vue-draggable-grid.mjs"
    },
    "./styles": {
      "import": "./dist/style.css",
      "default": "./dist/style.css"
    },
    "./*": "./*"
  },
  "scripts": {
    "dev": "vite",
    "build:watch": "vite build --watch",
    "build": "cross-env NODE_ENV=production vite build && tsc -d --emitDeclarationOnly && vue-typegen gen -s src -o dist",
    "preview": "vite preview",
    "lint:es": "eslint \"src/**/*.{js,ts,vue}\" --fix --color",
    "lint:style": "stylelint src/**/*.{css,scss,vue} --color --fix",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@interactjs/actions": "^1.10.20",
    "@interactjs/auto-start": "^1.10.20",
    "@interactjs/dev-tools": "^1.10.20",
    "@interactjs/interactjs": "^1.10.20",
    "@interactjs/modifiers": "^1.10.20",
    "@types/element-resize-detector": "^1.1.5",
    "element-resize-detector": "^1.2.4",
    "mitt": "^3.0.1",
    "vue": "^3.3.8"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@babel/types": "^7.23.0",
    "@types/jest": "^29.5.7",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "cross-env": "^7.0.3",
    "jsdom": "^22.1.0",
    "sass": "^1.69.5",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "vue-eslint-parser": "^9.3.2",
    "vue-template-compiler": "^2.7.15",
    "vue-tsc": "^1.8.22",
    "vue-typegen": "^0.2.0"
  }
}
